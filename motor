void setup() {
    for (int i = 8; i < 12; i++) {
        pinMode(i, OUTPUT);
        digitalWrite(i, LOW);
    }
}

void loop() {
  for (int i=8;i<12;i++){
    if (i==11){
      digitalWrite(8,HIGH);
    }
    else{
      digitalWrite(i+1,HIGH);
    }
    delay(5);
    digitalWrite(i,LOW);
    delay(5);
    }
 }
